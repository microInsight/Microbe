@model IEnumerable<Microbe.Models.ProjectInformationModels.ClientProjectReports>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.ClientProjectsID)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ReportParametersID)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ReportDefinitionsID)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.datafile)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ClientProjectsID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReportParametersID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReportDefinitionsID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.datafile)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
